                <h4 class="text-center">Pacjenci bez przypisanego lekarza: @withoutDoctor</h4>
                <hr />
                @await Html.PartialAsync("~/Views/Shared/MainList/_withoutDoctor.cshtml")
            </div>
            <br />

            <div class="withDoctor my-5 p-3 border shadow-lg rounded-3">
                <h4 class="text-center">Pacjenci w trakcie diagnostyki w SOR: @inProgress</h4>
                <hr />
                @await Html.PartialAsync("~/Views/Shared/MainList/_withDoctor.cshtml")
            </div>
            <br />

            <div class="admissionToDepartment my-5 p-3 border shadow-lg rounded-3">
                <h4 class="text-center">Pacjenci z ustalonym przyjęciem na oddział lub oczekujący na transport: @done</h4>
