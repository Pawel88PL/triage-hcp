@{
    ViewData["Title"] = "Lista";
}



<div class="container">
    <div class="row">
        <div class="col-8">
            <div>
                <div class="border shadow p-3 mb-5 bg-white rounded">
                    <h5>Pacjenci bez przypisanego lekarza:</h5>
                    @foreach (var pacjent in Model.Where(x => x.Color == "red"))
                    {
                        if (pacjent.Doctor == "Wybierz lekarza" & pacjent.Active == "Tak")
                        {
