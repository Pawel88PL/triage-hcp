@model IEnumerable<triage_hcp.Models.Patient>

<link rel="stylesheet" href="~/css/list.css" asp-append-version="true" />

@{
    ViewData["Title"] = "Lista";

    var withoutDoctor = Model.Where(x => x.DoctorId == null & x.IsActive == true).Count();

    var inProgress = Model.Where(x => !(x.DoctorId == null)
    & x.IsActive == true
    & x.WhatNext == "W trakcie diagnostyki SOR").Count();

    var done = Model.Where(x => !(x.DoctorId == null)
    & x.IsActive == true
    & !(x.WhatNext == "W trakcie diagnostyki SOR")).Count();

}

