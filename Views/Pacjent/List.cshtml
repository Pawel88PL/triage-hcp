                                               style="text-decoration:none; color:black">
                                                <h5>
                                                    <span class="animation">
                                                        @pacjent.Surname?.ToUpper() @pacjent.Name?.ToUpper() ||
                                                        @pacjent.DateTime.ToString("f")
                                                    </span>
                                                </h5>
                                                <p>@pacjent.Diagnosis</p>
                                                <p>
                                                    <b>@pacjent.Pesel</b> || @pacjent.Gender @pacjent.Age lat ||
                                                    sala: <b>@pacjent.Room</b> ||
                                                    do kogo: <b>@pacjent.ToWhomThePatient</b>
                                                </p>
                                            </a>
                                        </div>
                                    </div>


                                    <div class="col-4" style="padding-top: 30px">

                                        @using (Html.BeginForm("Details", "Edit", FormMethod.Post))
